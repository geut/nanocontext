{
  "name": "nanocontext",
  "version": "1.0.2",
  "description": "Small module to create a stateful context tree object to extend through decorators.",
  "main": "index.js",
  "files": [
    "errors.js",
    "index.js"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "jest --passWithNoTests",
    "posttest": "npm run lint",
    "lint": "xd-ns \"**/*.js\"",
    "version": "chan release --allow-yanked ${npm_package_version} && git add .",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "nanocustomassert": "^1.0.0",
    "nanoerror": "^1.0.0"
  },
  "devDependencies": {
    "@geut/chan": "^2.0.0",
    "@geut/xd": "^1.5.0",
    "standard": "^14.3.1",
    "jest": "^24.8.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "standard": {
    "env": [
      "jest",
      "node",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/nanocontext.git"
  },
  "keywords": [
    "geut",
    "module",
    "context"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/nanocontext/issues"
  },
  "homepage": "https://github.com/geut/nanocontext#readme",
  "publishConfig": {
    "access": "public"
  }
}
